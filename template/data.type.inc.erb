        private: VALUE <%=self[:var]%>;
	public: static VALUE get_<%=self[:var]%>(VALUE self){
	 	<%=self[:klass]%>* s;
	        Data_Get_Struct(self, <%=self[:klass]%>, s);
		return s-><%=self[:var]%>;
	 }
         static VALUE set_<%=self[:var]%>(VALUE self, VALUE newvalue){
	 	<%=self[:klass]%>* s;
	        Data_Get_Struct(self, <%=self[:klass]%>, s);
		rb_gc_mark(newvalue);
		s-><%=self[:var]%> = newvalue;
		return newvalue;
	 }

