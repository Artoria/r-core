        private: <%=self[:type]%> <%=self[:var]%>;
	public: static VALUE get_<%=self[:var]%>(VALUE self){
	 	<%=self[:klass]%>* s;
	        Data_Get_Struct(self, <%=self[:klass]%>, s);
		return Data_Wrap_Struct(G::<%=self[:type]%>, &<%=self[:type]%>::data_mark, &<%=self[:type]%>::data_free, &s-><%=self[:var]%>);
	 }
         static VALUE set_<%=self[:var]%>(VALUE self, VALUE newvalue){
	 	<%=self[:klass]%>* s;
	        Data_Get_Struct(self, <%=self[:klass]%>, s);
		<%=self[:type]%>* value;
		Data_Get_Struct(newvalue, <%=self[:type]%>, value);
		rb_gc_mark(newvalue);
		s-><%=self[:var]%> = *value;
		return newvalue;
	 }

